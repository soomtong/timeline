<div class="page-header">
    <h1>Details of timeline</h1>
</div>


<table class="table table-bordered">
    <tbody>
        <tr><th>timeAt</th><td><%= timeline.timeAt %></td></tr>
        <tr><th>title</th><td><%= timeline.title %></td></tr>
        <tr><th>description</th><td><%= timeline.description %></td></tr>
        <tr><th>media</th><td><%= timeline.media %></td></tr>
        <tr><th>caption</th><td><%= timeline.caption %></td></tr>    
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', path_to.edit_timeline(timeline), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', path_to.timeline(timeline), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', path_to.timelines()) %>
</div>