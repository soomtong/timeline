<div class="container main" id="timeline">

	<section>
		<time>2012,1,1</time>
		<h2>Development&nbsp;</h2>
		<article>
			<p><i><span class='c1'>Design</span> & <span class='c2'>Code</span><a href="/timelines">.</a></i></p>
		</article>
		<figure>						
			<img src="images/logo_zeston.png">
			<figcaption>logo</figcaption>
		</figure>
	</section>
	
	<ul>
		
		<!-- Event -->
		<% if (timelines.length > 0) { %>
			<% timelines.forEach(function (timeline) { %>
				<li>
					<time><%= timeline.timeAt %></time>
					<h3><%= timeline.title %></h3>
					<article>
						<p><%= timeline.description %></p>
					</article>
					<figure>						
						<img src="<%= timeline.media %>">
						<figcaption><%= timeline.caption %></figcaption>
					</figure>
				</li>
			<% }); %>
		<% } else { %>
		<li>
			<time>2012,4,30</time>								<!-- Event Date -->
			<h3>Headline Goes Here</h3>							<!-- Headline -->
			<article>											<!-- Main Text -->
				<p>The main text goes here. You can have p tags too.</p>
			</article>
			<figure>						
				<img src="images/externkeyword.png">			<!-- Media, can also be a link to youtube video etc (optional) -->
				<figcaption>Caption goes here.</figcaption>		<!-- Caption for media (optional) -->
			</figure>
		</li>
		<% } %>
		<!-- / Event -->
		
	</ul>

</div>